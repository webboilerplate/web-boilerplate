  "browserify": {
    "transform": [
      "browserify-shim",
      "babelify",
      "debowerify"
    ]
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "angular": {
      "exports": "angular",
      "depends": "jquery"
    }
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "gulp csslint && gulp jshint",
    "dist": "gulp deploy --production",
    "deploy": "gulp deploy --production",
    "start": "node ./lib/check.js && bower install && gulp default",
    "restart": "node ./lib/check.js && bower install && gulp watch --restart",
    "bower": "./node_modules/.bin/bower",
    "watch": "gulp watch"
  },
  "engines": {
    "node": ">=0.10.35"
  }
