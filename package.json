  "scripts": {
    "test": "npm run lint",
    "gulp": "gulp",
    "lint": "gnpm run sasslint && gulp jscs && npm run stylint",
    "sasslint": "gulp scsslint",
    "stylint": "stylint src/assets/stylus/app",
    "setup": "mkdir -p build/assets/js",
    "dist": "NODE_ENV=production gulp build && gulp dist && npm run dist-js",
    "dist-js": "BABEL_ENV=production browserify -e src/assets/js/main.js -t babelify | derequire > dist/public/assets/js/main.js",
    "stage": "NODE_ENV=production gulp build && gulp dist && npm run stage-js",
    "stage-js": "BABEL_ENV=stage browserify -e src/assets/js/main.js -t babelify | derequire > dist/public/assets/js/main.js",
    "dev": "npm run build && concurrently --kill-others \"npm run watch\" \"npm run watch-js\" \"npm run dev-server\"",
    "dev-server": "browser-sync start --no-ghost-mode --server 'build' --serveStatic 'src/static' --files 'build , src/static'",
    "build-js": "npm run setup && browserify --fast -e src/assets/js/main.js -d -t babelify | derequire > build/assets/js/main.js",
    "build": "gulp build && npm run build-js",
    "start": "npm run build && npm run dev",
    "watch-js": "watchify --poll=300 --fast -e src/assets/js/main.js -d -t babelify -o build/assets/js/main.js -v",
    "watch": "gulp watch"
  },
  "engines": {
    "node": ">=4.2.1"
  }
}
